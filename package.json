{
  "name": "bebop",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "build": "NODE_ENV=production NEXT_TELEMETRY_DEBUG=1 NEXT_TELEMETRY_DISABLED=1 next build",
    "export": "NODE_ENV=production NEXT_TELEMETRY_DEBUG=1 NEXT_TELEMETRY_DISABLED=1 next export",
    "compress": "gzip -9 -r -k ./out",
    "compile": "npm run build && npm run export && npm run compress",
    "start": "npm run client | npm run server",
    "client": "DEBUG=bebop:* NEXT_TELEMETRY_DEBUG=1 NEXT_TELEMETRY_DISABLED=1 next dev",
    "server": "DEBUG=bebop:* nodemon --watch ./server --watch index.js index.js",
    "production": "sudo NODE_ENV=production node index.js",
    "production:debug": "sudo NODE_ENV=production DEBUG=bebop:* node index.js",
    "service:start": "sudo systemctl start bebop.service",
    "service:stop": "sudo systemctl stop bebop.service",
    "service:reload": "npm run service:stop && npm run service:start",
    "service:link": "sudo ln -s /home/pi/app/current/etc/bebop.service /etc/systemd/system/bebop.service && sudo systemctl enable bebop.service",
    "service:unlink": "sudo systemctl disable bebop.service && sudo rm /etc/systemd/system/bebop.service",
    "remote:deploy": "pm2 deploy ecosystem.config.js production --force",
    "remote:publish": "pm2 deploy production exec \"git pull && npm run compile && npm run service:reload\"",
    "remote:push": "pm2 deploy production exec \"git pull && npm run service:reload\"",
    "remote:setup": "pm2 deploy ecosystem.config.js production setup",
    "lint": "eslint ./",
    "test": "TZ='GMT' mocha \"./{,!(node_modules)/**}/*.test.{js,jsx}\"",
    "reset": "git checkout -- . && git clean -fd && git status",
    "sync": "nodemon -e .js,.json,.jsx --exec \"rsync -av --delete --exclude-from=./.rsyncignore ./ pi@bebop.local:~/app/current\" || echo Stopped"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suprMax/bebop.git"
  },
  "author": "suprMax",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suprMax/bebop/issues"
  },
  "homepage": "https://github.com/suprMax/bebop#readme",
  "dependencies": {
    "@fontsource/roboto": "^4.1.0",
    "@serialport/parser-readline": "^9.0.1",
    "@svgr/webpack": "^5.4.0",
    "babel-plugin-styled-components": "^1.11.1",
    "clsx": "^1.1.1",
    "debug": "^4.3.1",
    "escape-string-regexp": "^4.0.0",
    "express": "^4.17.1",
    "i2c-bus": "^5.2.1",
    "jsmpeg-fast-player": "^0.2.0",
    "lodash": "^4.17.20",
    "next": "^10.0.0",
    "next-images": "^1.4.1",
    "oled-font-5x7": "^1.0.3",
    "oled-i2c-bus": "^1.0.12",
    "pigpio": "^3.2.4",
    "pngjs": "^6.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "^4.1.0",
    "rtttl-parse": "^1.3.1",
    "serialport": "^9.0.4",
    "styled-components": "^5.2.0",
    "styled-media-query": "^2.1.2",
    "styled-normalize": "^8.0.7",
    "tree-kill": "^1.2.2",
    "uni-config": "^3.0.1",
    "ws": "^7.4.2"
  },
  "devDependencies": {
    "@babel/register": "^7.12.10",
    "chai": "^4.2.0",
    "eslint": "^7.8.1",
    "eslint-config-nebenan": "^3.1.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "pm2": "^4.5.1"
  }
}
